{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "storybook/iframe-play",
	"title": "Iframe Block",
	"description": "iFrame Block with Play button",
	"icon": "video-alt3",
	"category": "storybook",
	"attributes": {
		"src": {
			"type": "string",
			"default": ""
		},
		"text": {
			"type": "string",
			"default": ""
		},
		"bgImageDesktop": {
			"type": "number",
			"default": 0
		},
		"bgImageMobile": {
			"type": "number",
			"default": 0
		}
	},
	"supports": {
		"multiple": true
	},
	"editorScript": "file:index.js",
	"editorStyle": "file:index.css",
	"style": "file:style-index.css",
	"viewScript": "file:script.js"
}
